def WWC (arr, input):
    i = 0
    while arr[i] != 99:
        if arr[i] % 10 == 1 or arr[i] % 10 == 2 or arr[i] % 10 == 4:
            #print("checking reader")
            arr = opReader(arr, arr[i], i)
            if arr[i] % 10 == 1 or arr[i] % 10 == 2:
                i += 4
                #print(arr[i], arr[i+1], arr[i+2], arr[i+3])
            elif arr[i] % 10 == 4:
                #print(arr[i], arr[i+1])
                i += 2
        if arr[i] == 3:
            #print("checking op3")
            #print(arr[i])
            arr = op3(arr, input, i)
            #print(arr[i], arr[i+1])
            i += 2
    return arr

def opReader(arr, opCode, i):
    param1 = (opCode // 100) % 10
    param2 = (opCode // 1000) % 10
    param3 = (opCode // 10000) % 10
    if opCode % 10 == 1:
        arr = BIGop1(arr, i, param1, param2, param3)
        #print("doing op1")
        #print(arr[i], arr[i+1], arr[i+2], arr[i+3])
    if opCode % 10 == 2:
        arr = BIGop2(arr, i, param1, param2, param3)
        #print("doing op2")
        #print(arr[i], arr[i+1], arr[i+2], arr[i+3])
    if opCode % 10 == 4:
        arr = BIGop4(arr, i, param1)
    #print(arr)
    return arr
    
def BIGop1 (arr, i, p1, p2, p3):
    if p1 == 0:
        add1 = arr[arr[i + 1]]
    else:
        add1 = arr[i+1]
    if p2 == 0:
        add2 = arr[arr[i + 2]]
    else:
        add2 = arr[i+2]
    if p3 == 0:
        arr[arr[i + 3]] = add1 + add2
    else:
        arr[i+3] = add1 + add2
    return arr

def BIGop2 (arr, i, p1, p2, p3):
    if p1 == 0:
        factor1 = arr[arr[i + 1]]
    else:
        factor1 = arr[i+1]
    if p2 == 0:
        factor2 = arr[arr[i + 2]]
    else:
        factor2 = arr[i+2]
    if p3 == 0:
        arr[arr[i + 3]] = factor1 * factor2
    else:
        arr[i+3] = factor1 * factor2
    return arr

def BIGop4(arr, i, p1):
    if p1 == 0:
        print(arr[arr[i+1]])
    else:
        print(arr[i+1])
    return arr

def op3(arr, input, i):
    setter = arr[i + 1]
    arr[setter] = input
    return arr


